{
  "name": "zhtrans",
  "displayName": "zhtrans",
  "description": "",
  "version": "0.0.2",
  "publisher": "kechenhh",
  "engines": {
    "vscode": "^1.73.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:zhtrans.trh",
    "onCommand:zhtrans.trj"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zhtrans.trh",
        "title": "翻译html"
      },
      {
        "command": "zhtrans.trj",
        "title": "翻译js"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "zhtrans.trh",
          "group": "navigation"
        },
        {
          "when": "editorHasSelection",
          "command": "zhtrans.trj",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "zhtrans",
      "properties": {
        "zhtrans.readDir": {
          "type": "string",
          "default": "D:\\zh.json",
          "description": "读取文件的路径"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}
